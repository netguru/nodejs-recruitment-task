{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base-ts",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "rules": {
    "@typescript-eslint/no-var-requires": 0
  }
}
