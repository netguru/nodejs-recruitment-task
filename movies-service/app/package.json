{
  "name": "movies-service",
  "author": "Adrian Chlubek",
  "license": "PROPERTIARY",
  "repository": "",
  "version": "1.0.0",
  "engines": {
    "node": ">=16.0.0"
  },
  "bin": {
    "nest": "node_modules/.bin/nest",
    "typeorm": "node_modules/.bin/typeorm"
  },
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:dist": "node dist/api/api",
    "lint": "eslint \"{src,test}/**/*.ts\" --max-warnings=0",
    "lint:fix": "eslint --fix \"{src,test}/**/*.ts\"",
    "prettier": "prettier --check \"**/*.((j|t)s|json|yml|yaml)\" --ignore-path .gitignore",
    "prettier:fix": "prettier --write \"**/*.((j|t)s|json|yml|yaml)\" --ignore-path .gitignore",
    "style:fix": "yarn prettier:fix && yarn lint:fix",
    "test": "mocha --config .mocharc.json",
    "t:m:r": "typeorm migration:run",
    "t:m:g": "typeorm migration:generate",
    "t:s:d": "typeorm schema:drop",
    "t:s:l": "typeorm schema:log"
  },
  "dependencies": {
    "@faker-js/faker": "6.*",
    "@nestjs/common": "8.*",
    "@nestjs/core": "8.*",
    "@nestjs/passport": "8.*",
    "@nestjs/platform-express": "8.*",
    "@nestjs/swagger": "5.*",
    "@nestjs/typeorm": "8.*",
    "axios": "0.*",
    "bcrypt": "5.*",
    "chalk": "^4.1.2",
    "class-transformer": "0.*",
    "class-validator": "0.*",
    "date-fns": "2.*",
    "jsonwebtoken": "^8.5.1",
    "mime": "1.*",
    "passport": "0.*",
    "pg": "8.*",
    "reflect-metadata": "0.*",
    "rxjs": "7.*",
    "swagger-ui-express": "4.*",
    "typeorm": "0.2.*",
    "typeorm-naming-strategies": "~4.1.0",
    "typescript": "4.*",
    "uuid": "8.*",
    "yaml": "~2.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "8.*",
    "@nestjs/schematics": "8.*",
    "@nestjs/testing": "8.*",
    "@trivago/prettier-plugin-sort-imports": "3.*",
    "@types/bcrypt": "5.*",
    "@types/chai": "^4.3.1",
    "@types/date-fns": "2.*",
    "@types/express": "4.*",
    "@types/faker": "~6.6.9",
    "@types/http-errors": "1.*",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "9.*",
    "@types/node": "~17.0.25",
    "@types/passport-strategy": "0.*",
    "@types/sinon": "^10.0.11",
    "@types/supertest": "2.*",
    "@types/uuid": "8.*",
    "@types/validator": "13.*",
    "@typescript-eslint/eslint-plugin": "5.*",
    "@typescript-eslint/parser": "5.*",
    "chai": "4.*",
    "eslint": "8.*",
    "eslint-config-prettier": "8.*",
    "eslint-plugin-import": "2.*",
    "eslint-plugin-prettier": "4.*",
    "js-yaml": "3.*",
    "mocha": "9.*",
    "prettier": "2.*",
    "sinon": "13.*",
    "supertest": "6.*",
    "ts-loader": "~9.2.8",
    "ts-node": "10.*",
    "tsconfig-paths": "3.*"
  }
}
